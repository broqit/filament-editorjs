{
    "name": "broqit/filament-editorjs",
    "description": "Next generation block styled editor.",
    "keywords": [
        "broqit",
        "laravel",
        "filament-editorjs"
    ],
    "homepage": "https://github.com/broqit/filament-editorjs",
    "license": "MIT",
    "authors": [
        {
            "name": "Rahman Ramsi",
            "email": "rahmanramsi19@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "spatie/laravel-package-tools": "^1.10",
        "illuminate/contracts": "^11.0",
        "filament/forms": "^3.2",
        "broqit/editorjs-simple-html-parser": "*",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "Broqit\\FilamentEditorJs\\": "src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Broqit\\FilamentEditorJs\\FilamentEditorJsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
